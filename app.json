{
  "expo": {
    "name": "rer-coach",
    "slug": "rer-coach",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.step.rercoach",
      "buildNumber": "0.20.2"
    },
    "android": {
      "package": "com.step.rercoach",
      "versionCode": 6,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "rercoach",
              "host": "coach.io",
              // "pathPrefix": "/records"
            },
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "description": "",
    "githubUrl": "https://github.com/Visheshk/rer-coaching/"
  }
}
