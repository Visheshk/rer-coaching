{
  "expo": {
    "name": "R.E.A.D.Y.",
    "slug": "rer-coach",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    
    "scheme": "rercoach",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 50000
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.step.rercoach",
      "buildNumber": "0.20.2"
    },
    "android": {
      "package": "com.step.rercoach",
      "versionCode": 6,
      "adaptiveIcon": { 
        "foregroundImage": "./assets/text.png",
        "backgroundImage": "./assets/background.png"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "rercoach",
              "host": "*.coach.io",
              "pathPrefix": "/*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "description": "",
    "githubUrl": "https://github.com/Visheshk/rer-coaching/"
  }
}
